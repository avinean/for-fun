{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true
  },
  "lib": ["es2015"],
  "paths": {
      "@/*": ["*"]
  },
  "include": [
    "src/**/*",
    "express.d.ts"
  ],
	"exclude": [
		"dist",
		"node_modules"
	]
}